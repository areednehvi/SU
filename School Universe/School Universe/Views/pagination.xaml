        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:School_Universe.Views"
        mc:Ignorable="d"
        Title="pagination" Height="555.508" Width="934.539">
    <Grid Margin="0,0,144.4,0.6">
        <DataGrid x:Name="dataGrid" VerticalAlignment="Top" Height="421" Margin="0,0,-135.2,0" HorizontalAlignment="Left" Width="882" SelectedItem="{Binding ChosenItem}" />
